<%- include heading.ejs %>
<%- include navbar.ejs %>

	<div class="container">
		<div class="row">
			<div class="col-sm-12 columns">
				<h1>Contacts</h1>
				<hr>
				<table class="table table-hover table-striped">
					<thead>
						<tr>
							<th>Firstname</th>
							<th>Lastname</th>
							<th>Occupation</th>
							<th>Employer</th>
						</tr>
					</thead>
					<tbody>
						<% contacts.forEach(function(contact) { %>
						<tr>
							<td><%= contact.firstName %></td>
							<td><%= contact.lastName %></td>
							<td><%= contact.occupation %></td>
							<td><%= contact.employer %></td>
						</tr>
						<% }); %>
					</tbody>
				</table>
			</div>
		</div>
	</div>
<%- include footer.ejs %>
